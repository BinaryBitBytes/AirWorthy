{
  "name": "airworthy",
  "version": "1.0.0",
  "description": "A full stack MERN application that uses an authentication system to allow FAA Inspectors, Airliners, and Aircraft Technicians to update progress on their work all while keeping a log of records on projects completed.",
  "main": "server/server.mjs",
  "quokka": {
    "babel": true,
    "js": true
  },
  "scripts": {
    "seed": "node -r esm server/src/data/seed.mjs",
    "start": "node -r esm server/server.mjs",
    "_start_": "node -r esm server/server.mjs --trace-deprecation ...",
    "_START_": "nodemon -r esm ts-node server.mjs --trace-deprecation ...",
    "STARTING": "node server/server.mjs",
    "START": "node --experimental-modules server/server.mjs",
    "start1": "node --experimental-modules server/server.mjs concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "envDebug": "cross-env NODE_OPTIONS=--trace-deprecation react-scripts start",
    "runStart": "react-scripts start",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "DEVELOP": "npm run develop --trace-deprecation",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build",
    "BUILD": "swc src --out-dir dist/src",
    "BUILD:ci": "tsc && npm run BUILD --trace-deprecation"
  },
  "keywords": [],
  "author": "Miles Col√≥n",
  "license": "MIT",
  "type": "module",
