    "node": "^19.4.0",
    "node-gyp": "^9.3.1",
    "nvm": "^0.0.4",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "sass": "^1.62.1",
