    "mongoose": "^7.1.1",
    "node": "^16.13.0",
    "node-gyp": "^9.3.1",
    "nodemon": "^3.0.1",
    "nvm": "0.0.4",
    "path": "^0.12.7",
    "react-icons": "^4.1.0",
    "react-router-dom": "^6.11.1",
